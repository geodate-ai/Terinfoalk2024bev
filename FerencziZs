    <script>
      // Kocka élhosszának megadása.
      Élhossz = 10;
      // A kiinduló adatk kiíratása.
      document.write("A számítandó kocka élhossza:", Élhossz);
      // Sortörés
      document.write("<br>");
      // Eredmények kiíratása
      document.write("A kocka térfogata:", Élhossz*Élhossz*Élhossz);
    </script>
  </body>
